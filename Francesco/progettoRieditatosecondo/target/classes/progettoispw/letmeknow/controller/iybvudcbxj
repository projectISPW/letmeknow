package progettoispw.letmeknow.controller;

import progettoispw.letmeknow.bean.UpdateDataBean;
import progettoispw.letmeknow.controller.utenti.utente123456;

public class UpdateDataController {
    private String userid;
    private utente123456 utente ;
    private UpdateDataBean bean;
    private Integer [] newArr;
    public UpdateDataController(String user, Integer [] input) {
        this.userid = user;
        this.newArr = input;
        utente=new utente123456(userid);
        utente.setData(newArr);
    }
}

package progettoispw.letmeknow.controller;

import progettoispw.letmeknow.bean.UpdateDescrizionePersonaleBean;
import progettoispw.letmeknow.controller.utenti.utente123456;

public class UpdateTagController {
    private String userid;
    private utente123456 utente ;
    private UpdateDescrizionePersonaleBean bean;
    private String newStr;
    public UpdateTagController(String user, String input) {
        this.userid = user;
        this.newStr = input;
        utente=new utente123456(userid);
        utente.setTag(newStr);
    }
}






